		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Magelearn_Categoryfaq::question" title="question" sortOrder="10">
					<resource id="Magelearn_Categoryfaq::question_save" title="Save question" sortOrder="10"/>
